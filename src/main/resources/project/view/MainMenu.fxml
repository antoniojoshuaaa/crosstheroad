<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id=""  prefHeight="780.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.view.MainMenuController">
   <children>
      <StackPane fx:id="stackPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="780.0" prefWidth="600.0">
         <children>
            <TilePane alignment="CENTER" hgap="8.0" orientation="VERTICAL" prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Text fill="#e5ff00" stroke="BLACK" strokeWidth="0.0" text="Street Crossing">
                     <font>
                        <Font name="Ink Free" size="58.0" />
                     </font>
                     <TilePane.margin>
                        <Insets bottom="50.0" />
                     </TilePane.margin>
                  </Text>
                  <Button cacheShape="false" mnemonicParsing="false" onAction="#handleStartGame" text="Start Game">
                     <font>
                        <Font name="Ink Free" size="24.0" />
                     </font></Button>
                  <Button mnemonicParsing="false" onAction="#handleHowToPlayButton" text="How to Play">
                     <font>
                        <Font name="Ink Free" size="24.0" />
                     </font></Button>
                  <Button mnemonicParsing="false" onAction="#handleExitGame" text="Exit Game">
                     <font>
                        <Font name="Ink Free" size="24.0" />
                     </font>
                  </Button>
               </children>
               <cursor>
                  <Cursor fx:constant="DEFAULT" />
               </cursor>
            </TilePane>
         </children>
      </StackPane>
   </children>
</AnchorPane>
